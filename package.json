{
  "name": "cannlytics_console",
  "productName": "Cannlytics Console",
  "version": "0.0.3",
  "license": "GPLv3",
  "private": true,
  "description": "Cannlytics Community Console.",
  "author": "Keegan Skeate <contact@cannlytics.com>",
  "build": {
    "productName": "Cannlytics Console"
  },
  "scripts": {
    "dev": "webpack-dev-server --env development",
    "livereload": "python manage.py livereload",
    "runserver": "python manage.py runserver",
    "build": "webpack --env production",
    "start": "npm run open && npm run serve && npm run open && npm run runserver && npm run livereload",
    "collectstatic": "python manage.py collectstatic --noinput",
    "publish": "npm run container && npm run cloud && npm run deploy",
    "container": "npm run collectstatic && gcloud builds submit --tag gcr.io/cannlytics/cannlytics-console",
    "cloud": "gcloud run deploy cannlytics-console --image gcr.io/cannlytics/cannlytics-console --region us-central1 --allow-unauthenticated --platform managed",
    "deploy": "firebase deploy --project cannlytics --only hosting:production",
    "lint": "python manage.py check",
    "open": "start cmd.exe /k"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "@primer/octicons": "^11.0.0",
    "bootstrap": "^5.0.0-beta1",
    "chart.js": "^2.9.4",
    "chartjs-chart-box-and-violin-plot": "^2.3.0",
    "chartjs-chart-matrix": "^0.1.3",
    "firebase": "^7.20.0",
    "frappe-charts": "^1.5.5",
    "idb": "^5.0.8",
    "particles.js": "^2.0.0",
    "popper.js": "^1.16.1",
    "style-loader": "^1.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "dotenv-webpack": "^6.0.0",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.1.0",
    "mini-css-extract-plugin": "^1.3.3",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-node-externals": "^2.5.2"
  }
}
