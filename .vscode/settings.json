{
  "css.lint.unknownProperties": "ignore",
  "editor.detectIndentation": false,
  "editor.glyphMargin": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.quickSuggestions": {
    "strings": true,
  },
  "files.eol": "\n",
  "files.watcherExclude": {
    "**/app/build/win-unpacked/**": true
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/__init__.py": true,
  },
  "git.enabled": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.autofetch": true,
  "git.decorations.enabled": false,
  "git.ignoreLimitWarning": true,
  "python.linting.pylintEnabled": true,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4  
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "foreground": "#0BA67F",
      "iconColour": "green",
      "rulerColour": "green",
      "rulerLane": "left"
    },
    "FIXME": {
      "foreground": "orange",
      "iconColour": "red",
      "rulerColour": "red",
      "rulerLane": "left"
    }
  },
  "cSpell.words": [
    "abcdefghijklmnopqrstuvwxyz",
    "allowlist",
    "analytes",
    "appspot",
    "bokeh",
    "boxplot",
    "cannlytics",
    "chartjs",
    "cloudbuild",
    "cloudrun",
    "cloudsql",
    "coas",
    "collectstatic",
    "csrf",
    "csrftoken",
    "daygrid",
    "djpass",
    "esmodules",
    "firebaseapp",
    "firestore",
    "fullcalendar",
    "gcloud",
    "gmail",
    "grotesk",
    "heatmap",
    "hmac",
    "hmacs",
    "keegan",
    "keychain",
    "lims",
    "linkedin",
    "noinput",
    "octicons",
    "plex",
    "popperjs",
    "postgres",
    "projectnum",
    "roadmap",
    "robohash",
    "runserver",
    "scrollable",
    "secretmanager",
    "skeate",
    "sqladmin",
    "staticfiles",
    "subcollection",
    "subcollections",
    "timegrid",
    "urandom",
    "uuid",
    "uuidv"
  ],
  "yaml.customTags": [
    "!ref"
  ],
  "npm-scripts.showStartNotification": false,
  "todo-tree.tree.showBadges": false
}